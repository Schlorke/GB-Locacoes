    "format:check": "prettier --check .",
    "format:md": "prettier --write \"**/*.md\"",
    "format:md:check": "prettier --check \"**/*.md\"",
    "lint": "eslint . --ext .js,.ts,.tsx --no-color --quiet || exit 0",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "lint:md": "markdownlint \"**/*.md\"",
    "lint:md:fix": "markdownlint \"**/*.md\" --fix",
    "fix:md": "pnpm format:md && pnpm lint:md:fix",
    "prepare": "husky",
    "quality:check": "pnpm format:check && pnpm lint && pnpm lint:md",
    "quality:fix": "pnpm format && pnpm lint:fix && pnpm fix:md",
    "start": "next start",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:storybook": "echo 'Storybook tests temporarily disabled'",
    "test:coverage": "vitest --coverage --run",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "openapi:generate": "next-openapi-gen",
    "openapi:generate-zod": "tsx scripts/generate-openapi-zod.ts",
    "openapi:dev": "next-openapi-gen --watch",
    "docs:dev": "pnpm dev",
    "design-system:build-tokens": "node scripts/build-design-tokens.js",
    "design-system:watch-tokens": "node scripts/watch-design-tokens-simple.js",
    "design-system:storybook": "pnpm storybook",
    "design-system:build": "pnpm build-storybook",
    "design-system:publish": "pnpm build-storybook",
    "design-system:lint": "pnpm lint && stylelint \"**/*.{css,scss}\"",
    "design-system:lint:fix": "pnpm lint:fix && stylelint \"**/*.{css,scss}\" --fix",
    "ci:test": "pnpm db:generate && pnpm lint && pnpm test && pnpm type-check",
    "ci:build": "pnpm db:generate && pnpm build-storybook",
    "ci:full": "pnpm db:generate && pnpm lint && pnpm test && pnpm type-check && pnpm build",
    "check:prisma": "node scripts/check-prisma.js",
    "preci:test": "pnpm check:prisma",
    "preci:build": "pnpm check:prisma",
    "ci:prisma-only": "pnpm db:generate && pnpm check:prisma",
    "fix:typescript": "node scripts/fix-typescript-errors.js",
    "ci:typescript-fix": "pnpm fix:typescript && pnpm type-check",
    "check:outdated-safe": "pnpm outdated | findstr /R \"@radix-ui @testing-library date-fns lucide-react\" || echo No outdated packages found",
    "update:safe": "pnpm update @radix-ui/react-* @testing-library/* date-fns lucide-react clsx",
    "verify:after-update": "pnpm run build && pnpm test",
    "diagnose:database": "node scripts/diagnose-database.js",
    "diagnose:connection": "node scripts/diagnose-connection.js",
    "supabase": "npx -y supabase"
  },
