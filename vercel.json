{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api-docs",
      "destination": "/api-docs"
    }
  ],
  "crons": [
    {
      "path": "/api/health",
      "schedule": "*/5 * * * *"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase-url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase-anon-key"
  }
}
