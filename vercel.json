{
  "buildCommand": "pnpm run build",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api-docs",
      "destination": "/api-docs"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/late-fees",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/expire-quotes",
      "schedule": "0 0 * * *"
    }
  ]
}
