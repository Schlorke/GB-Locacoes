{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "build": {
    "env": {},
    "installCommand": "pnpm install",
    "buildCommand": "npm run lint && npm run build"
  },
  "typescript": {
    "ignoreBuildErrors": true
  },
  "images": {
    "formats": ["image/avif", "image/webp"],
    "sizes": [16, 32, 48, 64, 96, 128, 256, 512]
  }
}
